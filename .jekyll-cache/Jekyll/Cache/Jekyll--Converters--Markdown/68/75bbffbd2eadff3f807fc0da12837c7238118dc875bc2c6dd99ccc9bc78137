<script type="text/javascript">

function populateThis(data){
	let imageURL = "";
	let expositorName = "";
	let topic = "";
	let bio = "";
	let templateText = ""
	let fullBioLink = "";

	let current = null;
	data = JSON.parse(data)
	for (let ii = 0; ii < data.length; ii++){
		current = data[ii]

		imageURL = current.Foto;
		expositorName = current.Nombre;
		topic = current.Tema;
		bio = current.Resumen;
		fullBioLink = current["Bio completa"];

		templateText += '<tr>\n    <td>';

'</td>\n</tr>';
		templateText += fullBioLink
		templateText += '</td>\n    <td>'
		templateText += imageURL;
		templateText += '" alt="" data-position="center center" />\n\t<div class="content">\n\t\t<div class="inner">\n\t\t\t<header class="major">\n\t\t\t\t<h3>';
		templateText += expositorName;
		templateText += '</h3>\n\t\t\t</header>\n\t\t\t<h6><b>Ponencia</b></h6>\n\t\t\t<header class="major">\n\t\t\t\t<h4 stlye="text-align:left;">'
		templateText += topic
		templateText += '</h4>\n\t\t\t</header>\n\t\t\t<p>'
		templateText += bio
		templateText += '</p>\n\t\t</div>\n\t</div>\n</section>\n'	
	}

	templateText += '</section>';
	document.querySelector('#future').innerHTML = templateText
}

loadJSON(populateThis, "https://gist.githubusercontent.com/seccperu/03c78edf094b280e6011648f9c0d3bbd/raw/a12f828b7b75afe8d42fdf15e054489be64e939e/ponentes.JSON");
</script>

